@model int[]
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link rel="stylesheet" href="/css/style.css">
</head>
<body>

    <h1>Here are some numbers!</h1>

<div>
    @foreach (var number in @Model)
    {
        <p>@number</p>
    }
</div>

</body>
</html>