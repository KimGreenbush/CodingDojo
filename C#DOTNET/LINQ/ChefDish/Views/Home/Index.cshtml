@using ChefDish.Models
@model List<Chef>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <div id="container">

    <nav>
        <h1>Chefs | <a href="dishes">Dishes</a></h1>
        <a href="new">Add a Chef</a>
    </nav>

    <h2>Check out our roster of Chefs!</h2>

    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th># of Dishes</th>
            </tr>
        </thead>
        <tbody>
                @foreach (Chef chef in @Model)
                {
            <tr>
                <td>@chef.FirstName @chef.LastName</td>
                <td>@chef.Age</td>
                <td>@chef.Dishes</td>
            </tr>
                }
        </tbody>
    </table>

    </div>
</body>
</html>